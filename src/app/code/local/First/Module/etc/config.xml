<?xml version="1.0" encoding="UTF-8" ?>
<config>
    <modules>
        <First_Module>
            <version>0.0.1</version>
        </First_Module>
    </modules>
    <default>
        <some>
            <random>
                <xpath>Here is a value.</xpath>
            </random>
        </some>
    </default>
    <stores>
        <french>
            <some>
                <random>
                    <xpath>Some other value.</xpath>
                </random>
            </some>
        </french>
    </stores>
    <global>
        <models>

            <sales>
                <rewrite>
                    <quote_address>First_Module_Model_Quote_Address</quote_address> <!-- rewrite of Mage_Sales_Model_Quote_Address -->
                </rewrite>
            </sales>

            <first>
                <class>First_Module_Model</class>
                <resourceModel>first_resource</resourceModel>
            </first>

            <cms_resource>
                <rewrite>
                    <page>First_Module_Model_Resource_Page</page> <!-- rewrite of Mage_Cms_Model_Resource_Page -->
                </rewrite>
            </cms_resource>

            <first_resource>

                <class>First_Module_Model_Resource</class>
                <entities>

                </entities>
            </first_resource>
        </models>
    </global>
    <models>

    </models>
</config>